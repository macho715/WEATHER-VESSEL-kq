{
  "version": 2,
  "name": "weather-vessel-static",
  "builds": [
    {
      "src": "public/logistics-app.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/logistics-app.html"
    },
    {
      "src": "/index.html",
      "dest": "/public/logistics-app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {},
  "buildCommand": "echo 'Static deployment - no build needed'"
}